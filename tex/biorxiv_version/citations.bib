pages = {1815--27},
pmid = {12130541},
publisher = {Cold Spring Harbor Laboratory Press},
title = {{C. elegans EOR-1/PLZF and EOR-2 positively regulate Ras and Wnt signaling and function redundantly with LIN-25 and the SUR-2 Mediator component.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12130541 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC186391},
volume = {16},
